<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--seekBar背景-->
    <item android:id="@android:id/background">
        <!--形状-->
        <shape android:shape="rectangle">
            <!--大小-->
            <size android:height="29dp" />
            <!--圆角-->
            <corners android:radius="2dp" />
            <!--背景-->
            <solid android:color="#24193D" />
            <!--边框-->
            <padding android:top="@dimen/dp_4" android:right="@dimen/dp_4" android:bottom="@dimen/dp_4" android:left="@dimen/dp_4"/>
            <stroke
                android:width="1dp"
                android:color="#C3C5C4" />
        </shape>
    </item>
    <!--seekBar的进度条-->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2dp" />
                <gradient android:type="linear"
                    android:startColor="#F275C4"
                    android:endColor="#FEC860"/>


            </shape>
        </clip>
    </item>
</layer-list>