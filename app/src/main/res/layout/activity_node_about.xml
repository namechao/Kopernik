<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@color/gray"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include
        layout="@layout/head_layout"/>

    <com.allen.library.SuperTextView
        android:id="@+id/modifyNodeLogoSpt"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        app:sBackgroundDrawableRes="@color/white"
        app:sLeftTextColor="@color/black"
        app:sLeftTextSize="15dp"
        app:sLeftTextString="@string/title_change_node_icon"
        app:sRightIconRes="@mipmap/icon_arrow"
        app:sRightIconWidth="10dp"
        app:sRightIconHeight="10dp"
        app:sDividerLineType="bottom"
        app:sDividerLineColor="@color/gray"
        app:sDividerLineHeight="1dp"
        app:sUseRipple="true">
    </com.allen.library.SuperTextView>


</LinearLayout>